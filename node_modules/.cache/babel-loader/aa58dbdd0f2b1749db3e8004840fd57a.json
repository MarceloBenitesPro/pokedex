{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nexport default {\n  name: 'App',\n\n  data() {\n    return {\n      pokemons: [],\n      search: \"\",\n      show_dialog: false,\n      selected_pokemon: null\n    };\n  },\n\n  mounted() {\n    axios.get(\"https://pokeapi.co/api/v2/pokemon?limit=151\").then(response => {\n      this.pokemons = response.data.results;\n    });\n  },\n\n  methods: {\n    get_id(pokemon) {\n      return Number(pokemon.url.split('/')[6]);\n    },\n\n    get_name(pokemon) {\n      return pokemon.name.charAt(0).toUpperCase() + pokemon.name.slice(1);\n    },\n\n    show_pokemon(id) {\n      axios.get(`https://pokeapi.co/api/v2/pokemon/${id}`).then(response => {\n        this.selected_pokemon = response.data;\n        this.show_dialog = !this.show_dialog;\n      });\n    },\n\n    get_move_level(move) {\n      for (let version of move.version_group_details) {\n        if (version.version_group.name == 'sword-shield' && version.move_learn_method.name == 'level-up') {\n          return version.level_learned_at;\n        }\n      }\n\n      return 0;\n    },\n\n    filtered_moves(pokemon) {\n      return pokemon.moves.filter(item => {\n        let include = false;\n\n        for (let version of item.version_group_details) {\n          console.log(version.version_group.name);\n\n          if (version.version_group.name == 'sword-shield' && version.move_learn_method.name == 'level-up') {\n            include = true;\n          }\n        }\n\n        return include;\n      });\n    }\n\n  },\n  computed: {\n    filtered_pokemons() {\n      return this.pokemons.filter(item => {\n        return item.name.includes(this.search);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoGA;AAIA;EACAA,WADA;;EAGAC;IACA;MACAC,YADA;MAEAC,UAFA;MAGAC,kBAHA;MAIAC;IAJA;EAMA,CAVA;;EAYAC;IACAC;MACA;IACA,CAFA;EAGA,CAhBA;;EAkBAC;IAEAC;MACA;IACA,CAJA;;IAKAC;MACA;IACA,CAPA;;IAQAC;MACAJ;QACA;QACA;MACA,CAHA;IAKA,CAdA;;IAeAK;MACA;QACA,oDACAC,4CADA,EACA;UACA;QACA;MACA;;MACA;IACA,CAvBA;;IAwBAC;MACA;QACA;;QACA;UACAC;;UACA,oDACAF,4CADA,EACA;YACAG;UACA;QAEA;;QACA;MACA,CAXA;IAYA;;EArCA,CAlBA;EAyDAC;IACAC;MACA;QACA;MACA,CAFA;IAGA;;EALA;AAzDA","names":["name","data","pokemons","search","show_dialog","selected_pokemon","mounted","axios","methods","get_id","get_name","show_pokemon","get_move_level","version","filtered_moves","console","include","computed","filtered_pokemons"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n    <v-app> \n    \n        <v-container>\n                  <v-container>\n                    <v-row>\n                      <v-container>\n                        <v-img\n                          :src=\"require('./assets/logo.png')\"\n                          class=\"my-3\"\n                          contain\n                          height=\"100\"\n                        />\n                      </v-container>\n                    </v-row>\n                   <v-text-field v-model= \"search\" \n                     labol=\"Pesquisar\" \n                     placeholder=\"procure pelo pokÃ©mon\" \n                     solo>    \n                    </v-text-field>\n                        <v-row >\n                        <v-col \n                        cols=\"2\"  \n                        v-for=\"pokemon in filtered_pokemons\" \n                        :key=\"pokemon.name\">\n                        <v-card  @click=\"show_pokemon(get_id(pokemon))\" >\n                          <v-container>\n                              \n                           <v-row  class=\"mx-0 d-flex justify-center \">\n                            <img :src=\"`https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/${get_id(\n                              pokemon\n                              )}.png`\" \n                              :alt=\"pokemon.name\" \n                            width=\"80%\">\n                            </v-row>\n                         <h2 class=\"text-center\">{{get_name(pokemon)}}</h2>\n                          </v-container>\n                        </v-card>\n                        </v-col>\n                        </v-row>\n                  </v-container>\n        </v-container>\n        <v-dialog\n      v-model=\"show_dialog\"\n      width=\"800\"\n    >\n      <v-card  v-if=\"selected_pokemon\" class=\"px-2\">\n       <v-container >\n         <v-row class=\"d-flex align-center\">\n           <v-col cols=4>\n                <img :src=\"`https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/${selected_pokemon.id}.png`\" \n                              :alt=\"selected_pokemon.name\" \n                            width=\"80%\">\n           </v-col>\n           <v-col cols=8>\n             <h1>{{ get_name(selected_pokemon) }}</h1>\n               <v-chip  label v-for=\"type in selected_pokemon.types\" \n               :key=\"type.slot\" class=\"mr-2\">\n               {{type.type.name}}\n               </v-chip>\n             <v-divider class=\"my-4\"></v-divider>\n             <v-chip  label >Altura : {{ selected_pokemon.height = 2.54 }}cm</v-chip>\n              <v-chip  label class=\"ml-2\" >Peso : {{ (selected_pokemon.weight * 0.45359237)\n                .toFixed(0) }}kgs</v-chip>\n           </v-col>\n         </v-row>\n             <h2>Moves</h2>\n             <v-simple-table>\n    <template v-slot:default>\n      <thead>\n        <tr >\n          <th class=\"text-left\">\n            Level\n          </th>\n          <th class=\"text-left\">\n            Name\n          </th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr\n         v-for=\"item in filtered_moves(selected_pokemon)\" \n         :key=\"item.move.name\"\n        >\n          <td>{{ get_move_level(item) }}</td>\n          <td>{{ item.move.name }}</td>\n        </tr>\n      </tbody>\n    </template>\n  </v-simple-table>\n       </v-container>\n      </v-card>\n    </v-dialog>\n    </v-app>\n    \n</template>\n\n\n<script>\n\nimport axios from \"axios\";\n\n\n\nexport default {\n  name: 'App',\n\n   data(){\n     return{\n       pokemons: [],\n       search: \"\",\n       show_dialog: false,\n       selected_pokemon: null,\n     }\n   },\n\n     mounted (){\n        axios.get(\"https://pokeapi.co/api/v2/pokemon?limit=151\").then((response)=> {\n           this.pokemons = response.data.results;\n        })\n     },\n     \n     methods: {\n\n      get_id(pokemon){\n          return  Number(pokemon.url.split('/')[6]);\n      },\n       get_name(pokemon){\n         return  pokemon.name.charAt(0).toUpperCase() + pokemon.name.slice(1);\n       },\n       show_pokemon(id){\n           axios.get(`https://pokeapi.co/api/v2/pokemon/${id}`).then((response)=> {\n             this.selected_pokemon = response.data;\n             this.show_dialog = !this.show_dialog;\n        })\n          \n       },\n       get_move_level(move){\n           for(let version of move.version_group_details){\n                 if(version.version_group.name == 'sword-shield' && \n              version.move_learn_method.name == 'level-up'){\n                  return version.level_learned_at;\n              }\n           }\n           return 0;\n       },\n       filtered_moves(pokemon){\n         return pokemon.moves.filter((item)=>{\n            let include = false;\n            for (let version of item.version_group_details){\n              console.log(version.version_group.name);\n              if(version.version_group.name == 'sword-shield' && \n              version.move_learn_method.name == 'level-up'){\n                include = true;\n              }\n                \n            }\n            return include;\n         })\n       }\n     },\n    computed: {\n      filtered_pokemons(){\n        return this.pokemons.filter((item) => {\n          return  item.name.includes(this.search);\n        })\n      }\n    }\n};\n\n</script>\n\n<style>\n#app {\n  background: linear-gradient(\n      to bottom right,\n      rgba(10, 10, 10, 1),\n       rgb(85, 2, 2)\n    )\n    no-repeat center center fixed !important;\n  -webkit-background-size: cover;\n  -moz-background-size: cover;\n  -o-background-size: cover;\n  background-size: cover !important;\n  background-position: center;\n  min-height: 100vh;\n}\n\n\n\n</style>\n"]},"metadata":{},"sourceType":"module"}